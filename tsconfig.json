{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "es6",
            "dom"
        ],
        "moduleResolution": "node",
        "types": [
            "cypress"
        ],
        "esModuleInterop": true,
        "allowJs": true,
        "noEmit": true,
        "strict": false,
        "skipLibCheck": true
    },
    "include": [
        "cypress/**/*.ts"
    ]
}